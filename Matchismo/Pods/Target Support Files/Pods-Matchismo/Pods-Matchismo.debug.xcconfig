GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/HotlineSDK"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/HotlineSDK" $(inherited) "${PODS_ROOT}/HotlineSDK/HotlineSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HotlineSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FDHotlineSDK" -framework "AVFoundation" -framework "AdSupport" -framework "AudioToolbox" -framework "CoreData" -framework "CoreMedia" -framework "Foundation" -framework "ImageIO" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods